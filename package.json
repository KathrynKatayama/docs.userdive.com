{
  "name": "@uncovertruth/docs.userdive.com",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@uncovertruth/textlint-config-ja": "^0.0.12",
    "@uncovertruth/tslint-config": "^0.0.1",
    "doctoc": "^1.2.0",
    "gitbook-cli": "^2.3.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^4.2.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-uglify": "^1.5.4",
    "markdownlint-cli": "^0.1.0",
    "mermaid": "^6.0.0",
    "npm-run-all": "^2.3.0",
    "require-dir": "^0.3.0",
    "textlint": "^7.1.0",
    "tslint": "^3.13.0",
    "typescript-require": "^0.2.9-1",
    "typings": "^1.3.2"
  },
  "scripts": {
    "build": "gitbook build && find _book -type l -delete && gulp build",
    "deploy": "npm run toc && npm run build",
    "lint:ja": "textlint -c @uncovertruth/textlint-config-ja ja",
    "lint:md": "markdownlint en ja",
    "lint:ts": "tslint gulpfile.ts gulp/**/*.ts",
    "lint": "npm-run-all -p lint:*",
    "postinstall": "gitbook install && typings install",
    "prebuild": "rm -rf _book && rm -rf dist",
    "start": "gitbook serve",
    "test": "npm run lint",
    "toc": "doctoc $(find . | grep md | grep -v 'node_modules\\|SUMMARY\\|GLOSSARY\\|LANGS\\|README\\|_TEMPLATE')"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uncovertruth/docs.userdive.com.git"
  },
  "keywords": [],
  "author": "UNCOVER TRUTH Inc.",
  "homepage": "http://docs.userdive.com/"
}
